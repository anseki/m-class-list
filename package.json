{
  "name": "m-class-list",
  "version": "1.1.3",
  "title": "mClassList",
  "description": "The super simple shim for `classList` of HTML and SVG, that transparently intercepts difference between modern browsers and semi-modern browsers.",
  "keywords": [
    "classlist",
    "HTML",
    "SVG",
    "shim",
    "polyfill",
    "contains",
    "toggle",
    "replace"
  ],
  "main": "m-class-list.min.js",
  "module": "./src/m-class-list.js",
  "jsnext:main": "./src/m-class-list.js",
  "files": [
    "m-class-list.min.js",
    "src/*.js",
    "!src/*.proc.*",
    "webpack.config.rules.js",
    "bower.json",
    "LICENSE-MIT"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.3",
    "jasmine-core": "^2.99.1",
    "log4js": "^2.5.3",
    "node-static-alias": "^1.0.0",
    "pre-proc": "^0.2.8",
    "pre-proc-loader": "^1.0.9",
    "skeleton-loader": "^1.1.5",
    "webpack": "^3.11.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack --display-modules",
    "test": "node ./test/httpd"
  },
  "homepage": "https://github.com/anseki/m-class-list",
  "repository": {
    "type": "git",
    "url": "git://github.com/anseki/m-class-list.git"
  },
